type Product {
    id:ID!
    name:String!
    price:Float!
}

type Query {
    allProducts:[Product!]!
    productById(id:ID!):Product
}

type Mutation {
    createProduct(name:String!, price:Float!):Product!
    updateProduct(id:ID!, name:String!, price:Float!):Product
    deleteProduct(id:ID!):String
}